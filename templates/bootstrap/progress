let h = ../html/package.dhall in
let List/map = http://prelude.dhall-lang.org/List/map in
let
  progressbar = \( bar : ../type/ProgressBar ) ->
    h.div
    [ h.class "progress-bar ${bar.pclass}"
    , h.role "progressbar"
    , h.style "width: ${Double/show bar.pprogress}%"
    , h.attr "aria-valuemin" "0"
    , h.attr "aria-valuenow" ( Double/show bar.pprogress )
    , h.attr "aria-valuemax" "100"
    ] h.nc
in
\( bars : List ../type/ProgressBar )
->
h.div
  [ h.class "progress" ]
  (List/map ../type/ProgressBar Text progressbar bars)